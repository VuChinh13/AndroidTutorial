<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".features.navigation.activityresultlauncher.FirstResultActivity">

    <Button
        android:id="@+id/btn_secondary"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:text="Chuyển sang màn 2"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/btn_secondary"
        app:layout_constraintEnd_toEndOf="@+id/btn_secondary"
        app:layout_constraintStart_toStartOf="@+id/btn_secondary" />
</androidx.constraintlayout.widget.ConstraintLayout>