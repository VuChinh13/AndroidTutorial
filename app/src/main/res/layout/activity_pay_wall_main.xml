<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".audioplayer.music.paywall.demobilling.ui.PayWallMainActivity">

    <Button
        android:id="@+id/btnPaywallSale30Yearly"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Paywall Sale 30% Yearly"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btnPaywallSale30Weekly" />

    <Button
        android:id="@+id/btnPaywallSale30Weekly"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Paywall Sale 30% Weekly"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btnPaywallSale50Weekly" />

    <Button
        android:id="@+id/btnPaywallSale50Weekly"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_60sdp"
        android:text="Paywall Sale 50% Weekly"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnPaywallOnboarding"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Paywall Onboarding"
        app:layout_constraintEnd_toEndOf="@id/btnPaywallSale30Yearly"
        app:layout_constraintStart_toStartOf="@id/btnPaywallSale30Yearly"
        app:layout_constraintTop_toBottomOf="@id/btnPaywallSale30Yearly" />

    <Button
        android:id="@+id/btnPaywallUnlockFeature"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Paywall Unlock Feature"
        app:layout_constraintEnd_toEndOf="@id/btnPaywallSale30Yearly"
        app:layout_constraintStart_toStartOf="@id/btnPaywallSale30Yearly"
        app:layout_constraintTop_toBottomOf="@id/btnPaywallOnboarding" />

    <Button
        android:id="@+id/btnPaywallFocusBottomSheet"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Paywall Focus BottomSheet"
        app:layout_constraintEnd_toEndOf="@id/btnPaywallSale30Yearly"
        app:layout_constraintStart_toStartOf="@id/btnPaywallSale30Yearly"
        app:layout_constraintTop_toBottomOf="@id/btnPaywallUnlockFeature" />

    <Button
        android:id="@+id/btnPaywallFocusFullScreen"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Paywall Focus FullScreen"
        app:layout_constraintEnd_toEndOf="@id/btnPaywallSale30Yearly"
        app:layout_constraintStart_toStartOf="@id/btnPaywallSale30Yearly"
        app:layout_constraintTop_toBottomOf="@id/btnPaywallFocusBottomSheet" />

</androidx.constraintlayout.widget.ConstraintLayout>